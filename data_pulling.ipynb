    "            lat.append(response[\"result\"][i][\"SHAPE\"].split()[1:][1].split(')')[0])\n",
    "            #print(f\"Processing Record {i} of URL {url_counter}\")\n",
    "        #url_counter += 1\n",
    "    \n",
    "    except(KeyError, IndexError):\n",
    "        print(f\"URL {url} not found. Skipping...\")\n",
    "\n",
    "#----END Loop-----\n",
    "\n",
    "# End of Log\n",
    "print(\"-----------------------------\")\n",
    "print(\"Data Retrieval Complete\")\n",
    "print(\"-----------------------------\") "
   ]
