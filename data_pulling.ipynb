   "id": "0c305123",
   "metadata": {},
   "outputs": [],
   "source": [
    "#List of endpoints for API\n",
    "complete_endpoints = []\n",
    "for i in range(0,len(start_str_list)):\n",
    "    complete_endpoints.append(f\"{start_str_list[i]}:{end_str_list[i]}\")"
   ]
